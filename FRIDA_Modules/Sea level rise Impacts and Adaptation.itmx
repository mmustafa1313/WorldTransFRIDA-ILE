				<element subscript="insufficient">
					<eqn>0.245</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.755</eqn>
				</element>
				<units>1</units>
			</aux>
			<stock name="Government\nDebt" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>B$</units>
			</stock>
