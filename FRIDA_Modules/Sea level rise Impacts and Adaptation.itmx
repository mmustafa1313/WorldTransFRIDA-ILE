				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  Fraction_of_original_asset_distribution_that_annualy_experiences_storm_surge_damage-Fraction_of_original_asset_distribution_that_experiences_storm_surge_damage_in_1980)*Asset_exposure_reduction_because_of_retreat</eqn>
				<units>1/year</units>
			</aux>
