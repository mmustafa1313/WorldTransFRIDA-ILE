	<model name="Coastal_population">
		<variables>
			<stock name="Coastal\npopulation" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Coastal_population_in_1980[Coastal_Protection_level]</eqn>
				<inflow>Population_growth_in_coastal_areas</inflow>
				<outflow>Annual_fatalities_due_to_coastal_floods</outflow>
				<outflow>Proactive_retreat_of_people_from_the_coast</outflow>
				<outflow>Reactive_retreat_of_people_from_the_coast</outflow>
				<units>Mp</units>
			</stock>
			<flow name="Population growth in\ncoastal areas">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Coastal_population[Run,Coastal_Protection_level]*population_growth_rate[Run]</eqn>
				<units>Mp/year</units>
			</flow>
			<flow name="Annual fatalities due to\ncoastal floods" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Number_of_people_flooded_annually_in_storm_surges[Run,Coastal_Protection_level]*Fatality_rate_in_flood_events[Run]</eqn>
				<units>Mp/year</units>
			</flow>
			<flow name="Proactive retreat of people from the coast" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Coastal_population * relative_annual_proactive_retreat_of_people / (1.0 - Total_removed_fraction_of_original_population_distribution )</eqn>
				<units>Mp/year</units>
			</flow>
			<aux name="Coastal\npopulation in 1980">
