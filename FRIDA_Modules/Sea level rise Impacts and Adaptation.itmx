			<aux name="Fraction of original people\ndistribution that annually\nexperiences storm surges">
				<doc>This function was fitted to DIVA data for different levels of effective flood height.</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(population_storm_exposure_max_fraction[Coastal_Protection_level] / (1.0 + EXP(-population_storm_exposure_logistic_growth_rate[Coastal_Protection_level] * (Net_global_flood_height - population_exposure_damage_halfpoint[Coastal_Protection_level]) ) )) + population_storm_exposure_baseline_value[Coastal_Protection_level]</eqn>
				<units>1/year</units>
